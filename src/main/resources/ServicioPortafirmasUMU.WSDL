<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:sp="urn:umu:eadmin:services:portafirmas" xmlns:sps="urn:umu:eadmin:services:portafirmas:schema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="ServicioPortafirmasUMU" targetNamespace="urn:umu:eadmin:services:portafirmas">
<types>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:cm="urn:umu:eadmin:services:common:schema" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soap11-enc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:sp="urn:umu:eadmin:services:portafirmas" xmlns:sps="urn:umu:eadmin:services:portafirmas:schema" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" elementFormDefault="qualified" targetNamespace="urn:umu:eadmin:services:portafirmas:schema">
<import namespace="urn:umu:eadmin:services:common:schema" schemaLocation="umu-eadmin-services-common_2_0.xsd"/>
<!--  TIPOS Y ELEMENTOS BASICOS  -->
<!--  Metadatos que debe proporcionar una aplicación que incluya documentación  -->
<complexType name="MetadatosBasicosType">
<annotation>
<documentation>Conjunto de metadatos que debe proporcionar una aplicación cuando incluye en la solicitud documentación relacionada que no se encuentra en Archivo.</documentation>
</annotation>
<sequence>
<element name="Nombre" type="string"/>
<element name="NivelDescripcion" type="sps:NivelDescripcionType"/>
<element name="FechaCreacion" type="dateTime"/>
<element name="TipoMime" type="string"/>
<element minOccurs="0" name="Otros" type="cm:AnyType"/>
</sequence>
</complexType>
<simpleType name="NivelDescripcionType">
<restriction base="string">
<enumeration value="EXPEDIENTE"/>
<enumeration value="DOCUMENTO"/>
</restriction>
</simpleType>
<!--  Unidad documental relacionada  -->
<complexType name="UnidadDocumentalRelacionadaType">
<annotation>
<documentation>Unidad documental a relacionar con una solicitud de firma. Consta de: - Identificador. Pid de una unidad documental de Archivo o el identificador de un documento gestionado por una aplicación. - Metadatos. Conjunto de metadatos que se deben incluir si el documento no procede de archivo. - DocumentacionExpediente. Documentación incluida en el expediente. No soportado.</documentation>
</annotation>
<sequence>
<element name="Identificador" type="string"/>
<element minOccurs="0" name="Metadatos" type="sps:MetadatosBasicosType"/>
<element minOccurs="0" name="DocumentacionExpediente" type="sps:DocumentacionRelacionadaType"/>
</sequence>
</complexType>
<!--  Posibles valores para el perfil de los firmantes de los flujos  -->
<simpleType name="PerfilFirmanteType">
<restriction base="anyURI">
<enumeration value="urn:umu:eadmin:services:portafirmas:perfilfirmante:personal"/>
<enumeration value="urn:umu:eadmin:services:portafirmas:perfilfirmante:institucional"/>
</restriction>
</simpleType>
<!--  Documentación relacionada con la solicitud  -->
<complexType name="DocumentacionRelacionadaType">
<annotation>
<documentation>Unidades documentales a relacionar con una solicitud de firma. - Procedencia (opcional). El valor "urn:umu:eadmin:servicios:archivo" indica que procede de Archivo, equivale al comportamiento por defecto.</documentation>
</annotation>
<sequence>
<element maxOccurs="unbounded" name="UnidadDocumentalRelacionada" type="sps:UnidadDocumentalRelacionadaType"/>
</sequence>
<attribute name="Procedencia" type="anyURI" use="optional"/>
</complexType>
<!--  Intervalo para las consultas de solicitudes  -->
<complexType name="IntervaloType">
<sequence>
<element minOccurs="0" name="FechaInicio" type="date"/>
<element minOccurs="0" name="FechaFin" type="date"/>
</sequence>
</complexType>
<!--  Información básica de solicitud  -->
<complexType name="InformacionBasicaSolicitudType">
<annotation>
<documentation>Información básica de una solicitud. Se utiliza en la respuesta a la consulta de solicitudes en cierto estado.</documentation>
</annotation>
<sequence>
<element name="IdSolicitud" type="string"/>
<element name="Asunto" type="string"/>
<element name="FechaCreacion" type="date"/>
<element name="FechaTramitacion" type="dateTime"/>
</sequence>
</complexType>
<!--  Documento base -->
<complexType name="DocumentoBaseType">
<sequence>
<element name="Contenido" type="cm:Base64DataType"/>
</sequence>
<attribute name="Codificacion" type="anyURI" use="optional"/>
</complexType>
<!--  DocumentoOriginal  -->
<element name="DocumentoOriginal">
<annotation>
<documentation>Documento a firmar que se incluye en la solicitud.</documentation>
</annotation>
<complexType>
<complexContent>
<extension base="sps:DocumentoBaseType">
<sequence>
<element name="Descripcion" type="string"/>
<!--  Nuevo Portafirmas 3.0.1  -->
<element minOccurs="0" name="TipologiaENI" type="string"/>
<element minOccurs="0" name="MetadatosList" type="sps:MetadatosListType"/>
</sequence>
</extension>
</complexContent>
</complexType>
</element>
<complexType name="MetadatosListType">
<sequence>
<element maxOccurs="unbounded" name="Metadato" type="sps:MetadatoType"/>
</sequence>
</complexType>
<complexType name="MetadatoType">
<sequence>
<element name="Clave" type="string"/>
<element name="Valor" type="string"/>
</sequence>
</complexType>
<!--  Revisor  -->
<complexType name="RevisorType">
<annotation>
<documentation>Revisor de un firmante.</documentation>
</annotation>
<sequence>
<element name="Identificador" type="string"/>
<element minOccurs="0" name="EstadoRevision" type="sps:EstadoRevisionType"/>
<element minOccurs="0" name="MotivoRechazo" type="string"/>
</sequence>
</complexType>
<simpleType name="EstadoRevisionType">
<restriction base="anyURI">
<enumeration value="urn:umu:eadmin:services:portafirmas:estado:revision:revisado"/>
<enumeration value="urn:umu:eadmin:services:portafirmas:estado:revision:rechazado"/>
<enumeration value="urn:umu:eadmin:services:portafirmas:estado:revision:sinaccion"/>
</restriction>
</simpleType>
<!--  Firmante  -->
<element name="Firmante" type="sps:FirmanteType"/>
<complexType name="FirmanteType">
<annotation>
<documentation>Usuario que se incluye como parte del flujo de firma asociado a una solicitud.</documentation>
</annotation>
<sequence>
<element name="Identificador" type="string"/>
<element minOccurs="0" name="Motivo" type="string"/>
<element minOccurs="0" name="Perfil" type="sps:PerfilFirmanteType"/>
</sequence>
</complexType>
<complexType name="EstadoFirmanteType">
<complexContent>
<extension base="sps:FirmanteType">
<sequence>
<element minOccurs="0" name="Estado" type="sps:EstadoFirmaType"/>
<element minOccurs="0" name="MotivoRechazo" type="string"/>
<!--  Nuevo v3.01: lista de revisores del firmante  -->
<element maxOccurs="unbounded" minOccurs="0" name="Revisor" type="sps:RevisorType"/>
<!--  No se devuelve para solicitudes creadas antes de la versión 3.02  -->
<element minOccurs="0" name="FechaFirmaRechazo" type="dateTime"/>
</sequence>
</extension>
</complexContent>
</complexType>
<simpleType name="EstadoFirmaType">
<restriction base="anyURI">
<enumeration value="urn:umu:eadmin:services:portafirmas:estado:firma:firmado"/>
<enumeration value="urn:umu:eadmin:services:portafirmas:estado:firma:rechazado"/>
<enumeration value="urn:umu:eadmin:services:portafirmas:estado:firma:sinaccion"/>
</restriction>
</simpleType>
<!--  Firmantes  -->
<complexType name="FirmantesType">
<sequence>
<element maxOccurs="unbounded" name="Firmante" type="sps:FirmanteType"/>
</sequence>
</complexType>
<!--  Paso de firma  -->
<complexType name="PasoFirmaType">
<sequence>
<element name="Descripcion" type="string"/>
<element name="Firmantes" type="sps:FirmantesType"/>
</sequence>
</complexType>
<complexType name="EstadoPasoFirmaType">
<sequence>
<element name="Descripcion" type="string"/>
<element maxOccurs="unbounded" name="Firmantes" type="sps:EstadoFirmanteType"/>
</sequence>
<attribute name="PasoCompletado" type="boolean" use="required"/>
</complexType>
<!--  Flujo de firma  -->
<element name="FlujoFirma" type="sps:FlujoFirmaType"/>
<complexType name="FlujoFirmaType">
<annotation>
<documentation>Flujo de firmantes de una solicitud.</documentation>
</annotation>
<sequence>
<element maxOccurs="unbounded" name="PasoFirma" type="sps:PasoFirmaType"/>
</sequence>
<attribute name="FlujoOrdenado" type="boolean" use="required"/>
<attribute name="RechazoPasoUnFirmante" type="boolean" use="optional"/>
</complexType>
<complexType name="EstadoFlujoFirmaType">
<annotation>
<documentation>Estado del flujo de firmantes de una solicitud.</documentation>
</annotation>
<sequence>
<element maxOccurs="unbounded" name="PasoFirma" type="sps:EstadoPasoFirmaType"/>
</sequence>
<attribute name="FlujoOrdenado" type="boolean" use="required"/>
</complexType>
<!--  Firma  -->
<complexType name="FechaFirmaType">
<simpleContent>
<extension base="dateTime">
<attribute name="Certificada" type="boolean" use="required"/>
</extension>
</simpleContent>
</complexType>
<complexType name="FirmaType">
<annotation>
<documentation>Información sobre el proceso de firma de una solicitud firmada.</documentation>
</annotation>
<sequence>
<element name="Formato" type="string"/>
<element minOccurs="0" name="Politica" type="string"/>
<element name="FechaFirma" type="sps:FechaFirmaType"/>
<element minOccurs="0" name="TSA" type="string"/>
<element ref="sps:FlujoFirma"/>
<element name="ResultadoVerificacion" type="string"/>
</sequence>
</complexType>
<!--  Información de seguridad  -->
<complexType name="InformacionSeguridadListType">
<sequence>
<element maxOccurs="unbounded" name="Firma" type="sps:FirmaType"/>
</sequence>
</complexType>
<!--  Información básica de documento  -->
<complexType name="InformacionBasicaDocumentoType">
<sequence>
<element minOccurs="0" name="Descripcion" type="string"/>
<element minOccurs="0" name="MetadatosList" type="sps:MetadatosListType"/>
<element minOccurs="0" name="InformacionSeguridadList" type="sps:InformacionSeguridadListType"/>
</sequence>
<attribute name="ValidacionId" type="string" use="optional"/>
</complexType>
<!--  Identificador de documento de archivo  -->
<complexType name="DocArchivoType">
<sequence>
<xsd:choice>
<element name="PidUnidadDocumental" type="sps:PidUnidadDocumentalType"/>
<element name="UuidUnidadDocumental" type="string"/>
</xsd:choice>
</sequence>
</complexType>
<!--  Identificador de documento de archivo  -->
<complexType name="PidUnidadDocumentalType">
<sequence>
<element name="Pid" type="string"/>
<element name="Estado" type="string"/>
</sequence>
</complexType>
<!--  Identificador de una solicitud  -->
<element name="IdSolicitud" type="string"/>
<!--  Localizador de un documento  -->
<element name="ValidacionId" type="string"/>
<!--  ELEMENTOS COMUNES  -->
<element name="EntradasOpcionales" type="cm:AnyType"/>
<!--  Entradas Opcionales  -->
<element name="SalidasOpcionales" type="cm:AnyType"/>
<element name="Resultado" type="cm:ResultType"/>
<!--  SOLICITUD INICIA SOLICITUD FIRMA  -->
<element name="IniciaSolicitudFirmaRequest" type="sps:IniciaSolicitudFirmaRequestType"/>
<complexType name="IniciaSolicitudFirmaRequestType">
<annotation>
<documentation>Petición para la creación de una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element name="Solicitante" type="string"/>
<element ref="sps:FlujoFirma"/>
<element name="Asunto" type="string"/>
<element minOccurs="0" name="DocumentacionRelacionada" type="sps:DocumentacionRelacionadaType"/>
<!--  Nuevo v3.01 - Envío de copias auténticas -->
<element minOccurs="0" name="EnvioCopiasAutenticas" type="sps:EnvioCopiasAutenticasType"/>
<!--  Nuevo v3.02 - Descripción de la solicitud  -->
<element minOccurs="0" name="Descripcion" type="string"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  Entradas opcionales -->
<element name="GeneraCopiaAutentica" type="boolean"/>
<element name="FirmaDispositivoMovil"/>
<element name="FechaLimite" type="dateTime"/>
<!--  Nuevo v3.01 Envío de copias auténticas	 -->
<complexType name="EnvioCopiasAutenticasType">
<annotation>
<documentation>Envío de copias autenticas.</documentation>
</annotation>
<sequence>
<element name="DescripcionCorreo" type="string"/>
<element name="Destinatarios" type="sps:DestinatariosType"/>
</sequence>
</complexType>
<!--  Nuevo v3.01 Envío de copias auténticas	 -->
<complexType name="DestinatariosType">
<annotation>
<documentation>Destinatarios de copias autenticas generadas.</documentation>
</annotation>
<sequence>
<element maxOccurs="unbounded" minOccurs="0" name="DestinatariosTo" type="sps:DestinatarioType"/>
<element maxOccurs="unbounded" minOccurs="0" name="DestinatariosCCO" type="sps:DestinatarioType"/>
</sequence>
</complexType>
<!--  Nuevo v3.01 Envío de copias auténticas	 -->
<complexType name="DestinatarioType">
<annotation>
<documentation>Destinatario de copias autenticas.</documentation>
</annotation>
<sequence>
<element name="CorreoElectronico" type="string"/>
</sequence>
</complexType>
<!--  RESPUESTA INICIA SOLICITUD FIRMA  -->
<element name="IniciaSolicitudFirmaResponse" type="sps:IniciaSolicitudFirmaResponseType"/>
<complexType name="IniciaSolicitudFirmaResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
<element minOccurs="0" ref="sps:IdSolicitud"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD MODIFICA SOLICITUD FIRMA  -->
<element name="ModificaSolicitudFirmaRequest" type="sps:ModificaSolicitudFirmaRequestType"/>
<complexType name="ModificaSolicitudFirmaRequestType">
<annotation>
<documentation>Petición para la modificación de una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element ref="sps:IdSolicitud"/>
<element name="Solicitante" type="string"/>
<element ref="sps:FlujoFirma"/>
<element name="Asunto" type="string"/>
<element minOccurs="0" name="DocumentacionRelacionada" type="sps:DocumentacionRelacionadaType"/>
<!--  Nuevo v3.01 - Tipos para el envío de copias  -->
<element minOccurs="0" name="EnvioCopiasAutenticas" type="sps:EnvioCopiasAutenticasType"/>
<!--  Nuevo v3.02 - Descripción de la solicitud  -->
<element minOccurs="0" name="Descripcion" type="string"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  Mismas entradas opcionales que iniciaSolicitudFirma  -->
<!--  Entradas opcionales
			<element name="GeneraCopiaAutentica" type="boolean"/>
			<element name="FirmaDispositivoMovil"/>
			<element name="FechaLimite" type="dateTime"/>  -->
<!--  RESPUESTA MODIFICA SOLICITUD FIRMA  -->
<element name="ModificaSolicitudFirmaResponse" type="sps:ModificaSolicitudFirmaResponseType"/>
<complexType name="ModificaSolicitudFirmaResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD GUARDA DOCUMENTO ORIGINAL  -->
<element name="GuardaDocumentoOriginalRequest" type="sps:GuardaDocumentoOriginalRequestType"/>
<complexType name="GuardaDocumentoOriginalRequestType">
<annotation>
<documentation>Petición para incluir un documento en una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element ref="sps:IdSolicitud"/>
<element ref="sps:DocumentoOriginal"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  Entradas opcionales  -->
<element name="VersionImprimible" type="cm:Base64DataType"/>
<!--  RESPUESTA GUARDA DOCUMENTO ORIGINAL  -->
<element name="GuardaDocumentoOriginalResponse" type="sps:GuardaDocumentoOriginalResponseType"/>
<complexType name="GuardaDocumentoOriginalResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
<element minOccurs="0" ref="sps:ValidacionId"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD INCLUYE DOCUMENTO ARCHIVO  -->
<element name="IncluyeDocumentoArchivoEnSolicitudRequest" type="sps:IncluyeDocumentoArchivoEnSolicitudRequestType"/>
<complexType name="IncluyeDocumentoArchivoEnSolicitudRequestType">
<annotation>
<documentation>Petición para incluir un documento de archivo en una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element ref="sps:IdSolicitud"/>
<element name="DocumentoArchivo" type="sps:DocArchivoType"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  RESPUESTA INCLUYE DOCUMENTO ARCHIVO  -->
<element name="IncluyeDocumentoArchivoEnSolicitudResponse" type="sps:IncluyeDocumentoArchivoEnSolicitudResponseType"/>
<complexType name="IncluyeDocumentoArchivoEnSolicitudResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD FINALIZA SOLICITUD FIRMA  -->
<element name="FinalizaSolicitudFirmaRequest" type="sps:FinalizaSolicitudFirmaRequestType"/>
<complexType name="FinalizaSolicitudFirmaRequestType">
<annotation>
<documentation>Petición para dar por finalizada la creación de una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element ref="sps:IdSolicitud"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  RESPUESTA FINALIZA SOLICITUD FIRMA  -->
<element name="FinalizaSolicitudFirmaResponse" type="sps:FinalizaSolicitudFirmaResponseType"/>
<complexType name="FinalizaSolicitudFirmaResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD CONSULTA LISTADO SOLICITUDES  -->
<element name="ListaSolicitudesFirmaRequest" type="sps:ListaSolicitudesFirmaRequestType"/>
<complexType name="ListaSolicitudesFirmaRequestType">
<annotation>
<documentation>Petición para obtener un listado de las solicitudes generadas en un determinado estado. La identificación del solicitante se determina por el elemento cm:ClaimedIdentity, cuya inclusión como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element name="Estado" type="anyURI"/>
<element minOccurs="0" name="Asunto" type="string"/>
<element minOccurs="0" name="IdFirmante" type="string"/>
<element minOccurs="0" name="IntervaloCreacion" type="sps:IntervaloType"/>
<element minOccurs="0" name="IntervaloTramitacion" type="sps:IntervaloType"/>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  RESPUESTA CONSULTA LISTADO SOLICITUDES  -->
<element name="ListaSolicitudesFirmaResponse" type="sps:ListaSolicitudesFirmaResponseType"/>
<complexType name="ListaSolicitudesFirmaResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element maxOccurs="unbounded" minOccurs="0" name="InformacionBasicaSolicitudFirma" type="sps:InformacionBasicaSolicitudType"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD CONSULTA ESTADO SOLICITUD  -->
<element name="ConsultaEstadoSolicitudFirmaRequest" type="sps:ConsultaEstadoSolicitudFirmaRequestType"/>
<complexType name="ConsultaEstadoSolicitudFirmaRequestType">
<annotation>
<documentation>Petición para obtener el estado de una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element ref="sps:IdSolicitud"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
<!--  Nuevo v3.01: El atributo DevuelveEstadoFlujoFirma esta obsoleto (DEPRECATED). Solo recupera el primer revisor del firmante.  -->
<attribute name="DevuelveEstadoFlujoFirma" type="boolean" use="optional"/>
<!--  Nuevo v3.01: Reemplaza al atributo DevuelveEstadoFlujoFirma y recupera todos los revisores del firmante.  -->
<attribute name="DevuelveFlujoFirmaCompleto" type="boolean" use="optional"/>
</complexType>
<!--  RESPUESTA CONSULTA ESTADO SOLICITUD  -->
<element name="ConsultaEstadoSolicitudFirmaResponse" type="sps:ConsultaEstadoSolicitudFirmaResponseType"/>
<complexType name="ConsultaEstadoSolicitudFirmaResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" name="FechaTramitacion" type="dateTime"/>
<element minOccurs="0" name="Motivo" type="string"/>
<element minOccurs="0" name="EstadoFlujo" type="sps:EstadoFlujoFirmaType"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<!--  SOLICITUD LISTA DOCUMENTOS EN SOLICITUD  -->
<element name="ListaDocumentosEnSolicitudFirmaRequest" type="sps:ListaDocumentosEnSolicitudFirmaRequestType"/>
<complexType name="ListaDocumentosEnSolicitudFirmaRequestType">
<annotation>
<documentation>Petición para obtener los documentos de una solicitud de firma. La inclusión del elemento cm:ClaimedIdentity como entrada opcional es obligatoria.</documentation>
</annotation>
<sequence>
<element minOccurs="0" ref="sps:EntradasOpcionales"/>
<element ref="sps:IdSolicitud"/>
</sequence>
<attribute name="Profile" type="anyURI" use="optional"/>
</complexType>
<!--  RESPUESTA LISTA DOCUMENTOS EN SOLICITUD  -->
<element name="ListaDocumentosEnSolicitudFirmaResponse" type="sps:ListaDocumentosEnSolicitudFirmaResponseType"/>
<complexType name="ListaDocumentosEnSolicitudFirmaResponseType">
<sequence>
<element ref="sps:Resultado"/>
<element minOccurs="0" ref="sps:SalidasOpcionales"/>
<element minOccurs="0" name="DocumentosEnSolicitudList" type="sps:DocumentosEnSolicitudListType"/>
</sequence>
<attribute name="Profile" type="anyURI" use="required"/>
</complexType>
<complexType name="DocumentosEnSolicitudListType">
<sequence>
<element maxOccurs="unbounded" name="DocumentoEnSolicitud" type="sps:DocumentoEnSolicitudType"/>
</sequence>
</complexType>
<complexType name="DocumentoEnSolicitudType">
<attribute name="ValidacionId" type="string" use="required"/>
<attribute name="Firmado" type="boolean" use="required"/>
</complexType>
<!--  SOLICITUD RECUPERA CONTENIDO DOCUMENTO  -->
<element name="RecuperaContenidoDocumentoRequest" type="sps:RecuperaContenidoDocumentoRequestType"/>
